{{!< default}}
{{#post}}
<article>
	{{>posts}}
	<div class="section-post wrap">
		<div class="post-wrap {{^if feature_image}}no-image{{/if}}{{#if @labs.members}}{{^if access}} no-access{{/if}}{{/if}}">
			{{#if @labs.members}}
			{{#if access}}
			{{content}}
			{{else}}
			{{>members_cta}}
			{{/if}}
			{{else}}
			{{content}}
			{{/if}}
		</div>
		{{#if @labs.members}}
		{{#if access}}
		{{>post_authors}}
		{{/if}}
		{{else}}
		{{>post_authors}}
		{{/if}}
		<div class="post-meta">
			{{>post_share}}
		</div>
	</div>
</article>
{{>post_navigation}}
<div class="section-disqus">
	{{#if @labs.members}}
	{{#if access}}
	{{>services/disqus}}
	{{/if}}
	{{else}}
	{{>services/disqus}}
	{{/if}}
</div>
{{#has tag="#subscribe-form"}}
{{>members_subscribe_form}}
{{/has}}
{{/post}}
